				reg = <2>;
				remote-endpoint = <&rgb_panel0_in_lcd0>;
			};
		};
	};
};

&dlcd2 {
	status = "disabled";
	panel = <&lvds_panel1>;
	ports {
		tcon4_out: port@1 {
			tcon4_out_panel: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&lvds_panel1_in_lcd2>;
			};
		};
